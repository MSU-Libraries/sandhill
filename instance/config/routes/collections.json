{
    "route": [
        "/collections",
        "/collections/"
    ],
    "template": "collections.html.j2",
    "data": [
        {
            "name": "item",
            "processor": "solr.select",
            "on_fail": 404,
            "params": {
                "q": "PID:*root",
                "fl": "PID, fgs_label_s",
                "sort": "fgs_label_s asc",
                "rows": 1000,
                "wt": "json"
            }
        }
    ]
}
