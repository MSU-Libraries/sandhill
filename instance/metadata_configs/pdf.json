            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_owner }}",
            "label": "Holding Institution",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.otherVersion }}",
            "label": "Other Versions",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.relatedMaterials_object_in_context }}",
            "label": "Related Items",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.rightsholder_name }}",
            "label": "Rights Holder",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.rightsholder_contact }}",
