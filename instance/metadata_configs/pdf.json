{
    "match_conditions": [
        {
            "evaluate": "{{ item['RELS_EXT_info:fedora/fedora-system:def/model#hasModel_uri_s'] }}",
            "match_when": ["info:fedora/islandora:sp_pdf"]
        }
    ],
    "title": "{{ item.fgs_label_s }}",
    "media_template": "media_display/pdf.html.j2",
    "display": [
        {
            "value": "{{ item.description }}",
            "label": "Summary",
            "metadata_template": "item_page_blocks/metadata_expand_block.html.j2"
        },
        {
            "value": "{{ item.collection_t }}",
            "label": "In Collections",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2",
            "link": "/{{ view_args.namespace }}"
        },
        {
            "value": "{{ item.genre_aat }}",
            "label": "Material Type",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_author }}",
            "label": "Authors",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2",
            "link": "/search?fq=name_author:{{ metadata_value | solr_escape | urlencode }}"
        },
        {
            "value": "{{ item.name_editor }}",
            "label": "Editors",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_contributor }}",
            "label": "Contributors",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_publisher }}",
            "label": "Publishers",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_signer }}",
            "label": "Signers",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_thesis_advisor }}",
            "label": "Thesis Advisors",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2",
            "link": "/search?fq=name_thesis_advisor:{{ metadata_value | solr_escape | urlencode  }}"
        },
        {
            "value": "{{ item.name_committee_member }}",
            "label": "Committee Members",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.name_degree_committee_member }}",
            "label": "Committee Members",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2",
            "link": "/search?fq=name_degree_committee_member:{{ metadata_value | solr_escape | urlencode }}"
        },
        {
            "value": "{{ item.date_key }}",
            "label": "Date",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.edition }}",
            "label": "Edition",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.series_info }}",
            "label": "Series",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.subject_display }}",
            "label": "Subjects",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2",
            "link": "/search?fq=subject_display:{{ metadata_value | solr_escape | urlencode }}"
        },
        {
            "value": "{{ item.subject_local_topic }}",
            "label": "Category",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.subject_tic }}",
            "label": "Turfgrass Thesaurus Terms",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item['ndltd.discipline'] }}",
            "label": "Program of Study",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2",
            "link": "/search?fq=ndltd.discipline:{{ metadata_value | solr_escape | urlencode }}"
        },
        {
            "value": "{{ item['ndltd.level'] }}",
