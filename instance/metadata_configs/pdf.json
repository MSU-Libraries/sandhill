            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.archived_version_url }}",
            "label": "Archived Version",
            "metadata_template": "item_page_blocks/metadata_descriptive_list.html.j2"
        },
        {
            "value": "{{ item.physical_location }}",
